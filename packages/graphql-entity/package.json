    "@oclif/config": "1.17.0",
    "@oclif/errors": "1.3.3",
    "@oclif/plugin-help": "3.2.0",
    "find-up": "4.1.0",
    "graphql": "15.3.0",
    "tslib": "2.0.3"
  },
  "devDependencies": {
    "@oclif/dev-cli": "1.22.2",
    "@types/node": "14.14.6",
    "globby": "11.0.1",
    "ts-node": "9.0.0"
  },
  "oclif": {
    "commands": "./dist/cli/commands",
    "bin": "gqle",
    "plugins": [
      "@oclif/plugin-help"
    ]
